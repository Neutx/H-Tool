  "_meta": {
    "models": {
      "restockQueueItems": "Items from canceled orders waiting for warehouse manager confirmation. Includes order context, product details, status (pending/received/damaged), and cancellation reason.",
      "inventoryAdjustments": "A record of inventory being added back to stock after an order cancellation. Tracks which product was restocked, the quantity added, the timestamp, which order it came from, the reason for adjustment, and who authorized it. Creates an audit trail for all inventory changes.",
      "productRestockRules": "Per-product configuration for restocking behavior (e.g., 'Never auto-restock' toggle). Merchants can set rules to prevent certain products from being automatically restocked when orders are canceled.",
      "failedSyncs": "Records of failed Unicommerce inventory sync attempts with error details and retry status. Helps diagnose integration issues and track sync reliability.",
      "products": "Product/SKU information from Shopify or Unicommerce inventory management. Contains product details, current stock levels, availability status, and inventory management settings. Used to determine if items can be restocked and to update availability after cancellations."
    },
    "relationships": [
      "Each RestockQueueItem references one Order and one Product",
      "Each InventoryAdjustment references one Product and one Order (if from cancellation)",
      "Each ProductRestockRule belongs to one Product",
      "FailedSync records reference the Product that failed to sync",
      "InventoryAdjustments create a complete audit trail of all stock changes"
    ]
  },
